<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <dimen name="main_iconScroll">70sp</dimen>
    <dimen name="main_iconScrollbar">70sp</dimen>
    <dimen name="main_iconweight">0.25</dimen>
    <dimen name="main_iconchoseweight">1</dimen>
    <style name="CustomTabText" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/colorunselectedicon</item>
    </style>
</resources>